<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="JMHD_USER_INFOEDIT" table="JMHD_USER_INFOEDIT">
		<id name="UUID" type="java.lang.String">
	    	<column name="UUID"/>
	    	<generator class="assigned" />
		</id>
		<property name="USER_ID" type="java.lang.String" length="12" />
		<property name="EMPIID" type="java.lang.String" length="32" />
		<property name="CONTACT" type="java.lang.String" length="20" />
		<property name="CONTACTPHONE" type="java.lang.String" length="20" />
		<property name="ADDRESS" type="java.lang.String" length="100" />
		<property name="CREATE_TIME" type="timestamp"  insert="false" update="false"/>
		<property name="STATUS" type="java.lang.String" length="1" />
		<property name="SEX" type="java.lang.String" length="10" />
		<property name="MPIID" type="java.lang.String" length="32" />
		<property name="IDCARD" type="java.lang.String" length="18" />
		<property name="LASTMODIFYDATE" type="timestamp"  />
		<property name="LASTMODIFYUSER" type="java.lang.String" length="32" />
		<property name="EMAIL" type="java.lang.String" length="100" />
		<property name="MANADOCTORID" type="java.lang.String" length="20" />
	</class>
</hibernate-mapping>