<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="JMHD_ADMIN_WORK" table="JMHD_ADMIN_WORK">
		<id name="WID" type="java.lang.String">
	    	<column name="WID"/>
	    	<generator class="assigned" />
		</id>
		<property name="DOCTOR_ID" type="java.lang.String" length="16" />
		<property name="STATUS" type="java.lang.String"  />
		<property name="TASKID" type="java.lang.String" length="32" />
		<property name="WTYPE" type="java.lang.String" length="30" />
		<property name="WURL" type="java.lang.String" length="500" />
		<property name="WTYPENAME" type="java.lang.String" length="50" />
		<property name="SQTASKID" type="java.lang.String" length="32" />
	</class>
</hibernate-mapping>