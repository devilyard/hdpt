<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="dc.domain.main.JmhdAdminWork" table="JMHD_ADMIN_WORK">
        <id name="wid" type="java.lang.Long">
            <column name="WID" precision="12" scale="0" />
            <generator class="sequence">
             <param name="sequence">JMHD_ADMIN_WORK_SQ</param>
            </generator>
        </id>
        <property name="doctorId" type="java.lang.String">
            <column name="DOCTOR_ID" length="16" />
        </property>
        <property name="status" type="java.lang.String">
            <column name="STATUS" length="1" />
        </property>
        <property name="taskid" type="java.lang.String">
            <column name="TASKID" length="32" />
        </property>
        <property name="wtype" type="java.lang.String">
            <column name="WTYPE" length="30" />
        </property>
        <property name="wurl" type="java.lang.String">
            <column name="WURL" length="500" />
        </property>
        <property name="wtypename" type="java.lang.String">
            <column name="WTYPENAME" length="50" />
        </property>
        <property name="sqtaskid" type="java.lang.String">
            <column name="SQTASKID" length="32" />
        </property>
    </class>
</hibernate-mapping>
