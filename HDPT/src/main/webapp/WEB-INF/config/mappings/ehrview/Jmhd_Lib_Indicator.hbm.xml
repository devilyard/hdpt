<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="dc.domain.diary.JmhdLibIndicator" table="JMHD_LIB_INDICATOR">
        <id name="id" type="java.lang.Integer">
            <column name="id" precision="5" scale="0" />
            <generator class="assigned" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="30" />
        </property>
        <property name="exp" type="java.lang.String">
            <column name="exp" length="500" />
        </property>
        <property name="status" type="java.lang.String">
            <column name="status" length="1" />
        </property>
        <property name="pid" type="java.lang.Integer">
            <column name="pid" precision="5" scale="0" />
        </property>
        <property name="units" type="java.lang.String">
            <column name="units" length="20" />
        </property>
        <property name="assemble" type="java.lang.String">
            <column name="is_assemble" length="1" />
        </property>
        <property name="formula" type="java.lang.String">
            <column name="formula" length="500" />
        </property>
    </class>
</hibernate-mapping>
