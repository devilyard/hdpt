<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="dc.domain.diary.JmhdRelatedDiaryIndicator" table="JMHD_RELATED_DIARY_INDICATOR">
        <composite-id name="id" class="dc.domain.diary.JmhdRelatedDiaryIndicatorId">
            <key-property name="diaryId" type="java.lang.Long">
                <column name="DIARY_ID" precision="12" scale="0" />
            </key-property>
            <key-property name="indicatorId" type="java.lang.String">
                <column name="INDICATOR_ID" length="32" />
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>
